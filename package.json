{
    "name": "copy-directory-path",
    "displayName": "copy-directory-path",
    "description": "Copies path of directory to clipboard",
    "version": "1.0.2",
    "publisher": "daniels",
    "repository": "https://github.com/DanielSundberg/copy-directory-path",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.copyDirectoryPath"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.copyDirectoryPath",
                "title": "Copy directory path"
            }
        ],
        "menus": {
            "editor/title/context": [
                {
                    "command": "extension.copyDirectoryPath"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/command-exists": "^1.2.0",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.66",
        "tslint": "^5.20.1",
        "typescript": "^2.6.1",
        "vscode": "^1.1.37"
    },
    "dependencies": {
        "@types/copy-paste": "^1.1.30",
        "command-exists": "^1.2.9",
        "copy-paste": "^1.3.0"
    }
}
